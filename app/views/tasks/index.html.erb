
<div class="container">
  <br>
  <center>
    
      <div class="row">
        
        <div class="col-md-8">
        <div class="thumbnail">
        <h3>Browse Latest Projects Submitted:</h3>
          <% @task.each do |task| %> 
          <hr>
          <p>Issue:  <%= link_to task.issue, npo_task_path(task.npo, task)  %></p>
          <p>Description: <%= task.description %></p>
          <% end  %> 
          </div>
        </div>
      
      
    </div>
  </center>
</div>